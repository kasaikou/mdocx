{
    "name": "github.com/streamwest-1629/mdocx Devcontainer",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "devcontainer",
    "workspaceFolder": "/workspace",
    "remoteUser": "vscode",
    "postCreateCommand": {
        "volumes": "sh -c \"sudo chown -R vscode .venv && sudo chown -R vscode node_modules\""
    },
    "postStartCommand": "yarn install",
    "customizations": {
        "vscode": {
            "extensions": [
                "eamodio.gitlens",
                "mhutchie.git-graph",
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "ms-python.python"
            ]
        }
    }
}